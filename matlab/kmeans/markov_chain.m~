close all

num_vec = csvread('num_vec.csv');
book_num = 1452 - 1;
num_books = 1;
par_inds = sum(num_vec(1:book_num))+1:sum(num_vec(1:book_num+num_books));
idx = csvread('kmeans_100_c_labels.csv');
idx = idx(par_inds);
idx = idx + 1;
x
m = max(x);
n = numel(x);
y = zeros(m,1);
p = zeros(m,m);
for k=1:n-1
    y(x(k)) = y(x(k)) + 1;
    p(x(k),x(k+1)) = p(x(k),x(k+1)) + 1;
end
p = bsxfun(@rdivide,p,y); p(isnan(p)) = 0;